{
    "$schema": "core://default.api/strict#",
    "title": "Strict JSON Schema to validate other JSON Schema",
    "id": "core://default.api/strict#",
    "type": "object",
    "extends": {
        "$ref": "http://json-schema.org/draft-04/schema#"
    },
    "required": ["title", "$schema", "type"],
    "properties": {
        "title": {
            "type": "string"
        },
        "$schema": {
            "type": "string"
        },
        "type": {
            "enum": ["array", "object"]
        },
        "properties": {
            "type": "object"
        }
    }
}
